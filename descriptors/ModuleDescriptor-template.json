{
  "id": "${artifactId}:${version}",
  "name": "MARCcat Module",
  "provides": [
    {
      "id": "marccat",
      "version": "1.0",
      "handlers": [
        {
          "methods": ["GET"],
          "pathPattern": "/search",
          "permissionsRequired": [],
