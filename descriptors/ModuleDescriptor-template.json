{
  "id": "${artifactId}-${version}",
  "name": "Cataloging",
  "requires": [
    {
      "id": "cataloging",
      "version": "1.0"
    }
  ],
  "provides": [
    {
      "id": "cataloging",
      "version": "1.0",
      "handlers": [
        {
          "methods": ["GET"],
          "pathPattern": "/record-templates",
          "permissionsRequired": []
        }
      ]
    },
    {
      "id": "_tenant",
      "version": "1.0",
      "interfaceType": "system",
      "handlers": [
        {
          "methods": [
            "POST"
          ],
          "pathPattern": "/_/tenant"
        },
        {
          "methods": [
            "DELETE"
          ],
          "pathPattern": "/_/tenant"
        }
      ]
    }
  ],
  "permissionSets": [],
  "launchDescriptor": {
    "dockerImage": "${artifactId}:${version}",
    "dockerArgs": {
      "HostConfig": { "PortBindings": { "8081/tcp":  [{ "HostPort": "%p" }] } }
    },
    "dockerPull" : false
  }
}
