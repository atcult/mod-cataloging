{
  "$schema" : "http://json-schema.org/draft-04/schema#",
  "title": "Field",
  "type": "object",
  "properties": {
    "fixed-field": {
      "type": "object",
      "properties": {
        "value": {
          "type": "string"
        }
      }
    },
    "variable-field": {
      "type": "object",
      "properties": {
        "categoryCode": {
          "type": "string"
        },
        "ind1" : {
          "type": "integer",
          "minimum": 0,
          "maximum": 9
        },
        "ind2" : {
          "type": "integer",
          "minimum": 0,
          "maximum": 9
        },
        "code": {
          "type": "string"
        },
        "subfields": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "code": {
                "type": "string",
                "pattern": "[a-z|0-9]"
              },
              "value": {
                "type": "string"
              }
            }
          }
        }
      }
    },
    "code": {
      "type": "string",
      "minLength": 3,
      "maxLength": 3
    }
  },
  "additionalProperties": false
}
