{
  "$schema" : "http://json-schema.org/draft-04/schema#",
  "title": "FieldTemplate",
  "type": "object",
  "properties": {
    "code": {
      "type": "string",
      "minLength": 3,
      "maxLength": 3
    },
    "ind1" : {
      "type": "integer",
      "minimum": 0,
      "maximum": 9
    },
    "ind2" : {
      "type": "integer",
      "minimum": 0,
      "maximum": 9
    },
    "value": {
      "type": "string"
    },
    "subfields": {
      "type": "array",
      "items": {
        "type": "string",
        "pattern": "[a-z|0-9]"
      }
    }
  },
  "additionalProperties": false
}