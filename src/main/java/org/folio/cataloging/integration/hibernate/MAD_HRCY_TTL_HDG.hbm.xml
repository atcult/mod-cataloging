<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 2.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping package="mades.hierarchy">
	<class name="HierarchyCacheTitle" table="MAD_HRCY_TTL_HDG">
		<id name="hdg_number">
			<column name="TTL_HDG_NBR" sql-type="NUMBER (11)"/>
			<generator class="assigned"/>
		</id>
		<property name="title">
			<column name="TTL_HDG_STRNG_TXT" />
		</property>
	</class>
</hibernate-mapping>