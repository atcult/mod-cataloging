<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 2.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping package="org.folio.cataloging.dao.persistence">
	<class name="LV_USER_GROUP" table="LV_USER_GROUP">
		<composite-id name="key" class="UserGroupKey">
			<key-property name="userNumber" column="USR_NBR" />
			<key-property name="groupNumber" column="GRP_NBR"/>
		</composite-id>
	</class>
</hibernate-mapping>
