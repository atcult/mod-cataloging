<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping package="librisuite.hibernate">
<class name="CLCTN_RULE_TMP"  schema="custom"  table="CLCTN_RULE_TMP">
    <composite-id>
    	<key-property name="idRule" 
            column="ID_RULE"/>
        <key-property name="idItem" 
            column="ID_ITEM"/>
        <key-property name="type" 
            column="TYP"/>
    </composite-id>
</class>
</hibernate-mapping>
