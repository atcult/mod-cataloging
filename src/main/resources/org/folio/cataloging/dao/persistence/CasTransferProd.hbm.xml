<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping package="com.casalini.hibernate.model">
<class   name="CasTransferProd"  schema="custom"  table="CAS_TRNSF_PRDCT">
    <composite-id>
        <key-property name="bibItemNumber" 
            column="BIB_ITM_NBR"/>
     </composite-id>    
    <property name="imported"> 
		<column name="IMPORTED"/>
	</property>
	<property name="booked"> 
		<column name="BOOKED"/>
	</property>
	<property name="transactionId"> 
		<column name="TRANSACTION_ID"/>
	</property>
 </class>
</hibernate-mapping>
