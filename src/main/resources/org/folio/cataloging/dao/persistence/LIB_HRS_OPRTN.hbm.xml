<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 2.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping package="org.folio.cataloging.dao.persistence">
	<class name="LIB_HRS_OPRTN" table="LIB_HRS_OPRTN">
		<id name="organisationNumber">
			<column name="ORG_NBR" not-null="true"/>
			<generator class="native">
				<param name="sequence">LIB_HRS_OPRTN_SEQUENCE</param>
			</generator>
		</id>
		<property name="mondayHoursOpeningTime">
			<column name="MONDAY_HRS_OPNG_TME" sql-type="DATE" not-null="true"/>
		</property>
		<property name="mondayHoursClosingTime">
			<column name="MONDAY_HRS_CLSG_TME" sql-type="DATE" not-null="true"/>
		</property>
		<property name="tuesdayHoursOpeningTime">
			<column name="TUESDAY_HRS_OPNG_TME" sql-type="DATE" not-null="true"/>
		</property>
		<property name="tuesdayHoursClosingTime">
			<column name="TUESDAY_HRS_CLSG_TME" sql-type="DATE" not-null="true"/>
		</property>
		<property name="wednesdayHoursOpeningTime">
			<column name="WEDNESDAY_HRS_OPNG_TME" sql-type="DATE" not-null="true"/>
		</property>
		<property name="wednesdayHoursClosingTime">
			<column name="WEDNESDAY_HRS_CLSG_TME" sql-type="DATE" not-null="true"/>
		</property>
		<property name="thursdayHoursOpeningTime">
			<column name="THURSDAY_HRS_OPNG_TME" sql-type="DATE" not-null="true"/>
		</property>
		<property name="thursdayHoursClosingTime">
			<column name="THURSDAY_HRS_CLSG_TME" sql-type="DATE" not-null="true"/>
		</property>
		<property name="fridayHoursOpeningTime">
			<column name="FRIDAY_HRS_OPNG_TME" sql-type="DATE" not-null="true"/>
		</property>
		<property name="fridayHoursClosingTime">
			<column name="FRIDAY_HRS_CLSG_TME" sql-type="DATE" not-null="true"/>
		</property>
		<property name="saturdayHoursOpeningTime">
			<column name="SATURDAY_HRS_OPNG_TME" sql-type="DATE" not-null="true"/>
		</property>
		<property name="saturdayHoursClosingTime">
			<column name="SATURDAY_HRS_CLSG_TME" sql-type="DATE" not-null="true"/>
		</property>
		<property name="sundayHoursOpeningTime">
			<column name="SUNDAY_HRS_OPNG_TME" sql-type="DATE" not-null="true"/>
		</property>
		<property name="sundayHoursClosingTime">
			<column name="SUNDAY_HRS_CLSG_TME" sql-type="DATE" not-null="true"/>
		</property>
		<property name="generalIndicator">
			<column name="GNRL_IND" sql-type="CHAR (1)"/>
		</property>
	</class>
</hibernate-mapping>